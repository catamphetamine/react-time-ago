{
	"presets": [
		"@babel/preset-env",
		"@babel/preset-react"
	],

	"env": {
		"development": {
			"plugins": [
				["@babel/plugin-transform-for-of", { loose: true }]
			]
		},
		"commonjs": {
			"plugins": [
				["@babel/plugin-transform-for-of", { loose: true }]
			]
		},
		"es6": {
			"presets": [
				"@babel/preset-react",
				["@babel/preset-env", { modules: false }]
			],
			"plugins": [
				["@babel/plugin-transform-for-of", { loose: true }]
			]
		}
	}
}